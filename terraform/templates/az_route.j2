module "{{ profile }}_{{ resource_name }}" {
  source             = "{{ module_source }}"
  name               = "{{ resource_name }}"
  resource_group = "{{ params['resource_group'] }}"
  route_table_name   = "{{ params['route_table_name'] }}"
  address_prefix     = "{{ params['address_prefix'] }}"
  next_hop_type      = "{{ params['next_hop_type'] }}"
}